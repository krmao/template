<resources>

    <!-- Base application theme. -->
    <style name="FinalAppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryDark">@color/primary_dark</item>
        <item name="colorAccent">@color/accent</item>
        <!--<item name="mergedAppBarLayoutStyle">@style/MergedAppBarLayoutStyle</item>-->
    </style>

    <style name="FinalAppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="FinalAppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <!--<style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Dark" />-->
    <style name="FinalAppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
